{
  "name": "aws-basic-api",
  "version": "1.0.0",
  "description": "NodeJS + MongoDB API for User Management, Authentication and Registration",
  "main": "dist/server.js",
  "scripts": {
    "ts:watch": "tsc -w",
    "dev:serve": "nodemon dist/server.js",
    "dev": "concurrently \"npm run ts:watch\" \"npm run dev:serve\"",
    "ts:build": "tsc",
    "prod:serve": "node dist/server.js",
    "start": "npm run ts:build && npm run prod:serve"
  },
  "engines": {
    "node": "8.12.0",
    "npm": "6.4.1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "express": "^4.17.3",
    "http-status-codes": "^1.3.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.4",
    "mongoose": "^5.9.2",
    "morgan": "^1.9.1",
    "request": "^2.88.0",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/config": "0.0.34",
    "@types/express": "^4.16.1",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/node": "^8.10.51",
    "concurrently": "^4.1.0",
    "tslint": "^5.11.0"
  }
}
